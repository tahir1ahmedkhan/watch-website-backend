{
  "name": "watch-store-backend",
  "version": "1.0.0",
  "description": "TypeScript backend for watch store with user authentication and order management",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "nodemon",
    "build": "tsc",
    "build:check": "tsc --noEmit",
    "build-and-start": "npm run build && npm start",
    "build-check": "node build-and-start.js",
    "test": "jest",
    "seed": "ts-node src/scripts/seedDatabase.ts",
    "seed:admin": "ts-node src/scripts/seedAdmin.ts",
    "setup": "node setup.js",
    "clean": "rimraf dist",
    "restart": "npm run build && npm run dev",
    "lint": "tsc --noEmit",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "typescript",
    "express",
    "mongodb",
    "jwt",
    "bcrypt"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@supabase/supabase-js": "^2.95.3",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.0.3",
    "multer": "^2.0.2"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/multer": "^2.0.0",
    "@types/node": "^20.10.4",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.3"
  }
}
